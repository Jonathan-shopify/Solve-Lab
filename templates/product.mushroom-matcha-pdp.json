            "heading_highlight_stencil": false,
            "heading_highlight_color": "rgba(0,0,0,0)",
            "heading_highlight_gradient": "linear-gradient(300deg, rgba(96, 165, 126, 1) 15%, rgba(249, 208, 163, 1) 100%)"
